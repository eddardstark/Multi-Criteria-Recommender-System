def get_score(text):
        positive = 0
	negative = 0
	"""normalize words by lowercasing and dropping non-alpha characters"""
	norm = lambda word: re.sub('[^a-z]', '', word.lower())

	"""create a list of lowercase words from the input text, and remove empty strings"""
	words = list(norm(word) for word in text.split() if word != '')

	"""process each word in the text"""
	for word in words:
		if word not in stop_words:
			if word in positive_words:
					positive += 1

			if word in negative_words:
					negative += 1
	return (positive,negative)

	
	
def get_features(text):
	positive_count = 0
	negative_count = 0
	word_count = 0
	onegram_score = 0
	onegram_count = 0
	bigram_score = 0
	bigram_count = 0
	character_count = 0
	_negative_count = 0
	_positive_count = 0
	service_negative_count = 0
	service_positive_count = 0
	room_positive_count = 0
	room_negative_count = 0

	"""normalize words by lowercasing and dropping non-alpha characters"""
	norm = lambda word: re.sub('[^a-z]', '', word.lower())

	"""create a list of lowercase words from the input text, and remove empty strings"""
	words = list(norm(word) for word in text.split() if word != '')
	
	"""build a list of bigrams by pairing words in the text"""
	bigrams = zip(words[0::2],words[1::2])+zip(words[1::2],words[2::2])

	"""process each word in the text"""
	for word in words:
		if word not in stop_words and word in onegram_positivity_index:
			onegram_score += onegram_positivity_index[word]
			onegram_count += 1
			word_count += 1
			if word in positive_words:
					positive_count += 1
			if word in negative_words:
					negative_count += 1


		if word not in stop_words and word in service_words:
                        #print re.findall(r"([^.]*?"+word+"[^.]*\.)",text)
			for sentence in text.split('.') :
			 if word in sentence:
			  _positive_count, _negative_count  = get_score(sentence)
			  service_positive_count = _positive_count
			  service_negative_count = _negative_count


		if word not in stop_words and word in room_words:
                        #print re.findall(r"([^.]*?"+word+"[^.]*\.)",text)
			for sentence in text.split('.'):
			 if word in sentence:
  			  _positive_count, _negative_count  = get_score(sentence)
			  room_positive_count = _positive_count
			  room_negative_count = _negative_count

	"""process each character in the text"""
	for character in text:
		character_count += 1

	"""Checks to prevent divide by zero errors"""
	if onegram_count == 0:
		onegram_score = 0
	else:
		onegram_score = int(onegram_score*1.0/onegram_count)

	return list([onegram_count,onegram_score,positive_count,negative_count,word_count,character_count,service_positive_count,service_negative_count,room_positive_count,room_negative_count,])

class ExtractFeatures(MRJob):
	"""Extract features from each row(review) in the dataset."""
	# The input is a json key value pair. The key being null
	DEFAULT_INPUT_PROTOCOL = 'json_value'

	def mapper(self, _, data):	
          try:
    		#data = json.loads(data)
		for i in data['Reviews']:

		  #extract star ratings and review text
		  #stars = data['stars'] 
		  stars = i['Ratings'].get('Overall')
		  text = i['Content']
		  service_rating = i['Ratings'].get('Service')
		  cleanliness_rating = i['Ratings'].get('Cleanliness')
   		  value_rating = i['Ratings'].get('Value')
		  rooms_rating = i['Ratings'].get('Rooms')
		  location_rating = i['Ratings'].get('Location')

		  #print value_rating 
		  #get a list of features for each review
		  #features = get_features(text)

		  #yield review_id as key and stars + features as values
		  print i['ReviewID'], (stars,  service_rating, cleanliness_rating, value_rating, rooms_rating, location_rating)
	  except:
		print >>sys.stderr, 'Could not load JSON object from stdin.'
	def reducer(self, review_id, features):
		#discard review id, and yield stars + features as key and null values.
		yield list(features), None

if __name__ == "__main__":
	ExtractFeatures().run()
